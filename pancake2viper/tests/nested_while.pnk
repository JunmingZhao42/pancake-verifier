fun simple() {
    /*@ ensures retval == 50 @*/
    var x = 0;
    var acc = 0;
    while (x < 100) {
        /*@ invariant 0 <= x && x <= 100 @*/
        /*@ invariant acc == x / 2 @*/
        if (x & 1 == 0) {
            continue;
        }
        acc = acc + 1;
        x = x + 1;
    }
    return acc;
}
