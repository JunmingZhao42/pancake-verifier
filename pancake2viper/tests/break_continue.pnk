fun nested() {
    /*@ ensures retval == 55 @*/
    var x = 0;
    var acc = 0;
    while (x < 100) {
        // /*@ invariant 0 <= x && x <= 100 @*/
        // /*@ invariant acc == x / 2 @*/
        if (x & 1 == 0) {
            continue;
        }

        if (x == 50) {
            break;
        }
        acc = acc + 1;
    }
    return acc;
}