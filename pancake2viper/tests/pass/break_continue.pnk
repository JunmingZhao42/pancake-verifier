fun nested() {
    /@ ensures retval == 55 @/
    var x = 0;
    var accu = 0;
    while (x < 100) {
        // /@ invariant 0 <= x && x <= 100 @/
        // /@ invariant accu == x / 2 @/
        if (x & 1 == 0) {
            continue;
        }

        if (x == 50) {
            break;
        }
        accu = accu + 1;
    }
    return accu;
}