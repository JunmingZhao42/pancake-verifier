fun main() {
    // st8 @base, 0;
    // stw @base + 1, (42 << 16) || (4 << 8) || 1;
    var x = < 10, 11 >;
    !stw @base, x;
    sussy();
    return 0;
}

fun sussy() {
    //var stuff = lds {1, 8, {1, 2}} @base + 66;
    //@print_int(0, stuff.0, 0, stuff.1.0);
    //@print_hex(0, 42, 0, 42);
    //@print_int(0, stuff.1.1, 0, stuff.1.2);
    var arr = lds 2 @base + 66;
    var fst = 0;
    var snd = 0;
    !ld8 fst, @base;
    !ld8 snd, @base + 8;
    @print_hex(0, arr.0, 0, arr.1);
    @print_hex(0, fst, 0, snd);
    return 0;
}